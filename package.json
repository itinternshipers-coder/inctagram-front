{
  "name": "inctagram-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:production": "npm run build",
    "start": "next start",
    "eslint": "eslint",
    "lint": "pnpm lint:js && pnpm lint:css",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:css:fix": "stylelint '**/*.{css,scss}' --fix",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:css:fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "icons:build": "node scripts/split-sprite-and-generate.mjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@reduxjs/toolkit": "^2.9.0",
    "clsx": "^2.1.1",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-redux": "^9.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-onboarding": "9.1.10",
    "@storybook/addon-vitest": "9.1.10",
    "@storybook/nextjs-vite": "9.1.15",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "9.1.10",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "storybook": "9.1.15",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
